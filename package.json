    "gulp-purgecss": "^2.3.0",
    "gulp-rev-all": "^2.0.3",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
