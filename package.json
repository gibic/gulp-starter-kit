  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "main": "gulpfile.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm-run-all clean --parallel _eleventy-watch _gulp-dev",
    "build": "npm-run-all clean --parallel _eleventy-build _gulp-build",
    "clean": "gulp clean",
    "_eleventy-watch": "eleventy --quiet --watch",
    "_eleventy-build": "eleventy",
    "_gulp-dev": "gulp --silent",
    "_gulp-build": "gulp build --production"
  },
  "keywords": [
    "gulp-starter-kit",
    "gulp 4",
    "css",
    "sass",
    "eleventy",
    "front-end",
    "web",
    "gulpjs",
    "rollup"
  ],
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "browser-sync": "^2.26.7",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
