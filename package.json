{
  "name": "gulp-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "Gulp Starter Kit for front-end developers.",
  "bugs": {
    "url": "https://github.com/nstungcom/gulp-starter-kit/issues",
    "email": "hi@nstung.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "license": "MIT",
  "author": "Son Tung Ngo <hi@nstung.com>",
  "main": "gulpfile.babel.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all clean --parallel eleventy:watch gulp:dev",
    "build": "npm-run-all clean --parallel eleventy:build gulp:build",
    "eleventy:watch": "eleventy --quiet --watch",
    "eleventy:build": "eleventy",
    "gulp:dev": "gulp --silent",
    "gulp:build": "cross-env NODE_ENV=production gulp build",
    "fix-css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "fix-js": "eslint \"src/**/*.js\" --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.1.0",
    "gulp-rev-all": "^2.0.2",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "js-beautify": "^1.10.3",
    "js-yaml": "^3.13.1",
    "markdown-it": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.14.2"
  }
}
